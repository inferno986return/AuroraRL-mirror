            ]
        },
        "2":{
            "id":2,
            "npcText":"Actually it does.",
            "replies":[
                {
                    "targetStatementId":3,
                    "replyText":"WHAT?!"
                }
            ]
        },
        "3":{
            "id":3,
            "npcText":"Not tomorrow, of course, but within next 10 years. Look at our report based on materials you collected.",
            "replies":[
                {
                    "targetStatementId":-1,
                    "replyText":"You must be kidding..."
                }
            ]
        }
    }
